  color: #f44336;
}

.attendance-badge.late {
  background: #fff3e0;
  color: #ff9800;
}

.attendance-badge.pending {
  background: #f5f5f5;
  color: #999;
}

.attendance-btn {
  padding: 0.5rem 1rem;
  border: none;
  border-radius: 5px;
  cursor: pointer;
  font-size: 1rem;
  transition: all 0.3s ease;
}

.attendance-btn:disabled {
  opacity: 0.5;
  cursor: not-allowed;
}

.attendance-btn.present {
  background: #4caf50;
  color: white;
}

.attendance-btn.present:hover:not(:disabled) {
  background: #45a049;
}

.attendance-btn.late {
  background: #ff9800;
  color: white;
}

.attendance-btn.late:hover:not(:disabled) {
  background: #fb8c00;
}

.attendance-btn.absent {
  background: #f44336;
  color: white;
}

.attendance-btn.absent:hover:not(:disabled) {
  background: #e53935;
}

/* Attendance Styles - Student */
.attendance-section {
  background: white;
  padding: 2rem;
  border-radius: 15px;
  box-shadow: 0 2px 20px rgba(0, 0, 0, 0.08);
  margin-bottom: 2rem;
}

.attendance-section h3 {
  display: flex;
  align-items: center;
  gap: 0.5rem;
  color: #e91e63;
  margin-bottom: 1.5rem;
}

.attendance-summary-student {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 1rem;
  margin-bottom: 2rem;
}

.attendance-stat-student {
  background: linear-gradient(135deg, #f5f7fa 0%, #c3cfe2 100%);
  padding: 1.5rem;
  border-radius: 10px;
  display: flex;
  align-items: center;
  gap: 1rem;
}

.attendance-stat-student i {
  font-size: 2rem;
}

.attendance-stat-student.present {
  background: linear-gradient(135deg, #e8f5e9 0%, #c8e6c9 100%);
}

.attendance-stat-student.present i {
  color: #4caf50;
}

.attendance-stat-student.absent {
  background: linear-gradient(135deg, #ffebee 0%, #ffcdd2 100%);
}

.attendance-stat-student.absent i {
  color: #f44336;
}

.attendance-stat-student.late {
  background: linear-gradient(135deg, #fff3e0 0%, #ffe0b2 100%);
}

.attendance-stat-student.late i {
  color: #ff9800;
}

.attendance-stat-student.total {
  background: linear-gradient(135deg, #e3f2fd 0%, #bbdefb 100%);
}

.attendance-stat-student.total i {
  color: #667eea;
}

.attendance-list-student {
  margin-top: 2rem;
  max-height: 600px;
  overflow-y: auto;
  padding-right: 0.5rem;
}

.attendance-list-student::-webkit-scrollbar {
  width: 6px;
}

.attendance-list-student::-webkit-scrollbar-track {
  background: #f1f1f1;
  border-radius: 10px;
}

.attendance-list-student::-webkit-scrollbar-thumb {
  background: #667eea;
  border-radius: 10px;
}

.attendance-list-student::-webkit-scrollbar-thumb:hover {
  background: #764ba2;
}

.attendance-list-student h4 {
  color: #333;
  margin-bottom: 1rem;
  font-size: 1.2rem;
}

.attendance-items {
  display: flex;
  flex-direction: column;
  gap: 1rem;
}

.attendance-item {
  display: grid;
  grid-template-columns: 2fr 2fr 1fr;
  align-items: center;
  gap: 1rem;
  padding: 1rem;
  background: #f8f9fa;
  border-radius: 8px;
  transition: all 0.3s ease;
}

.attendance-item:hover {
  background: #f0f0f0;
  transform: translateX(5px);
}

.attendance-date {
  display: flex;
  align-items: center;
  gap: 0.5rem;
  color: #555;
  font-weight: 500;
}

.attendance-date i {
  color: #667eea;
}

.attendance-class {
  display: flex;
  align-items: center;
  gap: 0.5rem;
  color: #666;
}

.attendance-class i {
  color: #e91e63;
}

.attendance-status {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 0.5rem;
  padding: 0.5rem 1rem;
  border-radius: 20px;
  font-weight: 600;
  font-size: 0.9rem;
}

.attendance-status.present {
  background: #e8f5e9;
  color: #4caf50;
}

.attendance-status.absent {
  background: #ffebee;
  color: #f44336;
}

.attendance-status.late {
  background: #fff3e0;
  color: #ff9800;
}

.no-attendance {
  text-align: center;
  padding: 3rem;
  color: #999;
}

.no-attendance i {
  font-size: 3rem;
  margin-bottom: 1rem;
  opacity: 0.3;
}

.no-attendance p {
  font-size: 1.1rem;
}

/* Responsive Admin */
@media (max-width: 1024px) {
  .overview-layout {
    grid-template-columns: 1fr;
  }

  .teacher-profile-section {
    position: relative;
    top: 0;
  }

  .form-grid {
    grid-template-columns: 1fr;
  }

  .weekly-calendar {
    grid-template-columns: repeat(2, 1fr);
  }

  .financial-summary {
    grid-template-columns: 1fr;
  }

  .revenue-chart {
    height: 250px;
    gap: 0.5rem;
  }

  .chart-bar {
    max-width: 50px;
  }

  .chart-value {
    font-size: 0.75rem;
  }

  .chart-legend {
    gap: 1rem;
  }

  .quick-info {
    grid-template-columns: repeat(2, 1fr);
  }
}

@media (max-width: 768px) {
  .admin-section-header {
    flex-direction: column;
    align-items: stretch;
  }

  .classes-grid {
    grid-template-columns: 1fr;
  }

  .students-table-container,
  .payments-table-container {
    overflow-x: scroll;
  }

  .admin-tabs {
    overflow-x: auto;
    white-space: nowrap;
  }

  .weekly-calendar {
    grid-template-columns: 1fr;
  }

  .quick-info {
    grid-template-columns: 1fr;
  }

  .schedule-title {
    font-size: 1.5rem;
  }

  .financial-summary {
    grid-template-columns: 1fr;
  }

  .revenue-chart {
    height: 220px;
    padding: 0.5rem;
  }

  .chart-bar {
    max-width: 40px;
  }

  .chart-value {
    font-size: 0.7rem;
  }

  .chart-label {
    font-size: 0.8rem;
  }

  .chart-legend {
    flex-wrap: wrap;
    gap: 0.8rem;
  }

  .legend-item {
    font-size: 0.8rem;
  }

  .student-payment-info {
    grid-template-columns: 1fr;
  }

  .attendance-summary,
  .attendance-summary-student {
    grid-template-columns: repeat(2, 1fr);
  }

  .attendance-controls {
    grid-template-columns: 1fr;
  }

  .attendance-item {
    grid-template-columns: 1fr;
    gap: 0.5rem;
  }

  .pagination-container {
    flex-direction: column;
    gap: 1rem;
  }

  .pagination-info {
    text-align: center;
    width: 100%;
  }

  .pagination-controls {
    width: 100%;
    justify-content: center;
    overflow-x: auto;
  }

  .pagination-btn {
    min-width: 32px;
    height: 32px;
    font-size: 0.85rem;
    padding: 0 0.5rem;
  }

  .calendar-dropdown {
    min-width: 280px;
    left: 50%;
    transform: translateX(-50%);
  }

  .calendar-month {
    font-size: 0.95rem;
  }

  .weekday {
    font-size: 0.75rem;
    padding: 6px 2px;
  }

  .date-cell {
    font-size: 0.85rem;
  }
}
