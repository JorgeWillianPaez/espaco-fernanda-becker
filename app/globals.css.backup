}

.calendar-footer {
  margin-top: 1rem;
  padding-top: 0.75rem;
  border-top: 2px solid #f0f0f0;
  display: flex;
  justify-content: center;
}

.calendar-today-btn {
  background: linear-gradient(135deg, #e91e63, #9c27b0);
  color: white;
  border: none;
  padding: 10px 24px;
  border-radius: 20px;
